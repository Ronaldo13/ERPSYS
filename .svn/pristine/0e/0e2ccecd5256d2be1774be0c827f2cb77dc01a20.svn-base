package com.hqyj.model.hth;

import java.util.Date;

public class ErpsysExpenditure {
    private Integer expenditureId;

    private Double expenditureMoney;

    private String expenditureReason;

    private Date expenditureTime;
    
    private String expenditureTimeString;

    public Integer getExpenditureId() {
        return expenditureId;
    }

    public void setExpenditureId(Integer expenditureId) {
        this.expenditureId = expenditureId;
    }

    public Double getExpenditureMoney() {
        return expenditureMoney;
    }

    public void setExpenditureMoney(Double expenditureMoney) {
        this.expenditureMoney = expenditureMoney;
    }

    public String getExpenditureReason() {
        return expenditureReason;
    }

    public void setExpenditureReason(String expenditureReason) {
        this.expenditureReason = expenditureReason;
    }

    public Date getExpenditureTime() {
        return expenditureTime;
    }

    public void setExpenditureTime(Date expenditureTime) {
        this.expenditureTime = expenditureTime;
    }

	public String getExpenditureTimeString() {
		return expenditureTimeString;
	}

	public void setExpenditureTimeString(String expenditureTimeString) {
		this.expenditureTimeString = expenditureTimeString;
	}
    
    
}